{
  "name": "react-tailwindcss",
  "version": "1.0.0",
  "description": "Create React App with Tailwind CSS",
  "license": "MIT",
  "author": {
    "name": "Amit Agarwal",
    "email": "amit@labnol.org",
    "url": "https://www.labnol.org"
  },
  "private": true,
  "scripts": {
    "prestart": "postcss src/tailwind.css -o src/index.css",
    "start": "react-scripts start",
    "prebuild": "postcss src/tailwind.css -o src/index.css --env production",
    "build": "react-scripts build",
    "inline": "npm run build && gulp"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "tailwindui",
    "vscode",
    "create-react-app"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.2.0",
    "@tailwindcss/ui": "^0.1.3",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "gulp": "^4.0.2",
    "gulp-inline-source": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rename": "^2.0.0"
  }
}
